package br.com.livraria.modelo;

public class Livro extends Produto{
	private String editora;
	private String autor;
	private int isbn;
	
	public Livro() {}

	public Livro(String editora, String autor, int isbn) {
		super();
		this.editora = editora;
		this.autor = autor;
		this.isbn = isbn;
	};
	
	public void SetAll(String editora, String autor, int isbn) {
		super();
		this.editora = editora;
		this.autor = autor;
		this.isbn = isbn;
	};
	
}
